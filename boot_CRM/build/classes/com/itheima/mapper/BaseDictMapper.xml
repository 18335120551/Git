<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.mapper.BaseDictMapper">
	<sql id="baseDict">
		select * from base_dict
	</sql>
	<!-- 查询字典表 -->
	<select id="findBaseDictByCode" parameterType="String"
		resultType="BaseDict">
		<include refid="baseDict" />
		<where>
			dict_type_code = #{value};
		</where>
	</select>
</mapper>